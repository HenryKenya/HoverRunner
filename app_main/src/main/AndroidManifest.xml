<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.usehover.testerv2">

	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.READ_PHONE_STATE"/>
	<uses-permission android:name="android.permission.CALL_PHONE"/>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


	<application
	    android:allowBackup="true"
	    android:icon="@mipmap/ic_launcher"
	    android:label="@string/app_name"
	    android:name=".ApplicationInstance"
	    android:supportsRtl="true"
	    android:theme="@style/myHoverTheme">
		<meta-data
				android:name="com.hover.ApiKey"
				android:value="847346e4e73beb57a1186f69fedfbfb4"/>

		<activity
		    android:name=".MainActivity"
		    android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".ui.login.LoginActivity" />
		<activity android:name=".ui.webview.WebViewActivity" />
        <activity android:name=".ui.actions.filter.ActionFilterActivity" android:theme="@style/AppTheme" />
		<activity android:name=".ui.transactions.filter.TransactionFilterActivity" android:theme="@style/AppTheme" />
		<activity android:name=".ui.action_details.ActionDetailsActivity" />
		<activity android:name=".ui.parsers.ParsersActivity" />
		<activity android:name=".ui.transactionDetails.TransactionDetailsActivity" />
        <activity android:name=".Fakesdk" />
        <activity android:name=".ui.actions.uncompletedVariables.UncompletedVariableActivity" />
    </application>

</manifest>